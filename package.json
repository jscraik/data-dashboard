{
  "name": "data-behavior-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check src src-tauri/src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "cli:score": "cargo run --manifest-path src-tauri/Cargo.toml --bin behavior-scorer",
    "watcher": "tsx src/session-watcher.ts",
    "watcher:scan": "tsx src/session-watcher.ts --scan",
    "watcher:report": "tsx src/session-watcher.ts --report"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@tauri-apps/plugin-sql": "^2.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.454.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@tailwindcss/vite": "^4.1.0",
    "@tauri-apps/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.0",
    "glob": "^13.0.3",
    "jsdom": "^26.0.0",
    "tailwindcss": "^4.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "vite": "^6.4.1",
    "vitest": "^3.0.0"
  }
